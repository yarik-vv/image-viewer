<!DOCTYPE html>
<html lang="en">

<head>
  <meta http-equiv="X-UA-Compatible" content="IE=edge" />
  <meta charset="UTF-8">
  <meta name="viewport" content="width=device-width, initial-scale=1" />
  <title>Test work</title>
  <% if(NODE_ENV==='development') { %>
    <link rel="stylesheet" href="styles.css">
  <% } %>
</head>

<body>
  <header>
   <h3>Image view</h3>
  </header>
  <div class="content">
    <main>
      <div class="images-list" id="list"></div>
      <div class="pagination"></div>
    </main>
    <aside class="filter">
      <h3>Filter by</h3>
      <div class="size">
        <h4>Size</h4>
        <input type="radio" name="gender" value="male">Large<br>
        <input type="radio" name="gender" value="female">Medium<br>
        <input type="radio" name="gender" value="other">Small
      </div>
      <div class="author">
        <h4>Author</h4>
      </div>
    </aside>
  </div>
</body>
</html>